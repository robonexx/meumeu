datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id       String   @id @map("_id") @default(auto()) @db.ObjectId
  title    String?  
  content  String
  date     DateTime @default(now())
  category String
  author   String
  likes    Int      @default(0)
}
